{
  "$id": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "name": "Keyfairy API Gateway",
  "port": 8080,
  "extra_config": {
    "router": {
      "return_error_msg": true
    }
  },
  "endpoints": [
    {
      "endpoint": "/users",
      "method": "POST",
      "output_encoding": "no-op",
      "backend": [
        {
          "host": ["http://auth-service:8000"],
          "url_pattern": "/api/v1/users/",
          "encoding": "no-op"
        }
      ]
    },
    {
      "endpoint": "/users",
      "method": "GET",
      "output_encoding": "no-op",
      "backend": [
        {
          "host": ["http://auth-service:8000"],
          "url_pattern": "/api/v1/users",
          "encoding": "no-op"
        }
      ]
    },
    {
      "endpoint": "/users/{uid}",
      "method": "GET",
      "output_encoding": "no-op",
      "backend": [
        {
          "host": ["http://auth-service:8000"],
          "url_pattern": "/api/v1/users/{uid}",
          "encoding": "no-op"
        }
      ]
    },
    {
      "endpoint": "/users/{uid}",
      "method": "PUT",
      "output_encoding": "no-op",
      "backend": [
        {
          "host": ["http://auth-service:8000"],
          "url_pattern": "/api/v1/users/{uid}",
          "encoding": "no-op"
        }
      ]
    },
    {
      "endpoint": "/auth/register",
      "method": "POST",
      "output_encoding": "no-op",
      "backend": [
        {
          "host": ["http://auth-service:8000"],
          "url_pattern": "/api/v1/auth/register",
          "encoding": "no-op"
        }
      ]
    },
    {
      "endpoint": "/auth/login",
      "method": "POST",
      "output_encoding": "no-op",
      "backend": [
        {
          "host": ["http://auth-service:8000"],
          "url_pattern": "/api/v1/auth/login",
          "encoding": "no-op"
        }
      ]
    },
    {
      "endpoint": "/auth/refresh-token",
      "method": "POST",
      "output_encoding": "no-op",
      "backend": [
        {
          "host": ["http://auth-service:8000"],
          "url_pattern": "/api/v1/auth/refresh-token",
          "encoding": "no-op"
        }
      ]
    },
    {
      "endpoint": "/practice/register",
      "method": "POST",
      "output_encoding": "no-op",
      "backend": [
        {
          "host": ["http://receiving-service:8110"],
          "url_pattern": "/api/v1/practice/register",
          "encoding": "no-op"
        }
      ]
    },
    {
      "endpoint": "/postural-errors/practice/{practice_id}",
      "method": "GET",
      "output_encoding": "no-op",
      "backend": [
        {
          "host": ["http://video-analysis-service:8100"],
          "url_pattern": "/api/v1/postural-errors/practice/{practice_id}",
          "encoding": "no-op"
        }
      ]
    }
  ]
}